<UserControl x:Class="NeuralAction.WPF.SetupGuideOther"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:NeuralAction.WPF"
             mc:Ignorable="d">
    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="DarkStyle.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </UserControl.Resources>
    <Grid>
        <Image Source="/NeuralAction.WPF;component/Resources/setup_setting.png" Width="772" Height="406"/>
        <CheckBox Margin="70,138,0,0" HorizontalAlignment="Left" VerticalAlignment="Top" IsChecked="{Binding UseGPU}"/>
        <CheckBox Margin="70,221,0,0" HorizontalAlignment="Left" VerticalAlignment="Top" IsChecked="{Binding AllowControl}"/>
    </Grid>
</UserControl>
